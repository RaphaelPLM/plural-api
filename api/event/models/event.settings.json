{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "capa": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "body": {
      "type": "richtext"
    },
    "photo_credit": {
      "type": "string"
    },
    "horario": {
      "type": "datetime"
    },
    "prioridade": {
      "type": "integer",
      "default": 999999
    }
  }
}
